{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@model/(.*)$": ["<rootDir>/../src/model/$1.entity"],
    "^@service/(.*)$": ["<rootDir>/../src/module/$1.service"],
    "^@controller/(.*)$": ["<rootDir>/../src/module/$1.controller"],
    "^@module/(.*)$": ["<rootDir>/../src/module/$1.module"],
    "^@validator/(.*)$": ["<rootDir>/../src/validator/$1.validator"],
    "^@config/(.*)$": ["<rootDir>/../src/config/$1.config"],
    "^@helper/(.*)$": ["<rootDir>/../src/common/helper/$1.helper"]
  }
}
